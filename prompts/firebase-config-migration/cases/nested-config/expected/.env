# Firebase Functions Configuration - Nested Config Example
# Copy this file to .env.local for local development

# Database Configuration
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=myapp_db
DATABASE_CREDENTIALS_USER=db_user
DATABASE_CREDENTIALS_PASSWORD=secure_password_here

# External Services Configuration
SERVICES_TWILIO_ACCOUNT_SID=your_twilio_sid_here
SERVICES_TWILIO_AUTH_TOKEN=your_twilio_token_here
SERVICES_SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Feature Flags Configuration
FEATURES_NOTIFICATIONS_ENABLED=true
FEATURES_NOTIFICATIONS_CHANNELS=email,sms,slack
FEATURES_ANALYTICS_ENABLED=true